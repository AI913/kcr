scheme("JobOrder")
output_directory(ENV["FASTLANE_BUILD_OUTPUT_PATH"])
output_name("JobOrder.ipa")
archive_path(ENV["FASTLANE_BUILD_OUTPUT_PATH"] + "/JobOrder.xcarchive")
clean(true)

for_lane :deploy do
  export_method 'app-store'
  configuration 'Release'
end

for_lane :stage do
  export_method 'ad-hoc'
  configuration 'AdHoc'
end
